{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "src/",
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "lib": ["DOM", "es2019"],
    "target": "es6"
  },
  "exclude": ["node_modules"],
  "typeAcquisition": {
    "enable": true
  }
}
